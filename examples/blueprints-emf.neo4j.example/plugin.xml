<plugin
      name="%pluginName"
      id="blueprints-emf.neo4j.example"
      version="1.0.0"
      provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.eclipselabs.org/socnet"
            class="org.eclipselabs.example.socnet.SocnetPackage"
            genModel="model/socnet.genmodel"/>
   </extension>

</plugin>
